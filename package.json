{
  "name": "miter",
  "version": "0.1.1",
  "description": "A typescript web framework based on ExpressJs based loosely on SailsJs",
  "scripts": {
    "test": "tsc ; node ./dist/test/index.js",
    "build": "tsc",
    "watch": "tsc -w",
    "prepublish": "tsc",
    "lint": "tslint -c \"config/tslint.json\" --project \"tsconfig.json\" -e \"**/*.spec.ts\" -t stylish",
    "pre-commit": "bash ./scripts/pre-commit.sh"
  },
  "pre-commit": [
    "pre-commit"
  ],
  "main": "./dist/index.js",
  "typings": "./dist/index",
  "dependencies": {
    "@types/body-parser": "^0.0.33",
    "@types/cli-color": "^0.3.29",
    "@types/express": "^4.0.33",
    "@types/express-jwt": "^0.0.33",
    "@types/lodash": "^4.14.37",
    "@types/node": "^7.0.0",
    "@types/pluralize": "^0.0.27",
    "@types/sequelize": "^4.0.38",
    "body-parser": "^1.15.2",
    "cli-color": "^1.1.0",
    "debug": "^2.2.0",
    "express": "^4.14.0",
    "express-jwt": "^5.1.0",
    "lodash": "^4.16.4",
    "mysql": "^2.11.1",
    "pluralize": "^3.0.0",
    "pre-commit": "^1.2.2",
    "reflect-metadata": "^0.1.8",
    "sequelize": "^3.24.6"
